class User
{
  name;
  id; 
}

class Applicant
{
  isA User;
  cGPA;
  skills;  
  Boolean isGraduate;
  
  1 applicant <@>- 0..3 Application applications;
  1 applicant <@>- 0..3 Assignment assignments;
}

class Instructor
{
  isA User;
  1 <@>- * Hours contactTimes;
}

class Department
{
  isA User;
  Date deadline;
}

class Assignment
{
  autounique id;
  feedback;  
}

class Application 
{
  autounique id;
  Boolean isAccepted;
  experience;
  courseGPA;
}

class Job
{
  Integer maxHours;
  Double wage;
  Date deadline;
  requiredSkills;
  requiredCourseGPA;
  requiredCGPA;
  requiredExperience;
  1 job <@>- 0..1 Assignment assignment;
  1 job <@>- * Application applications;
}

class TAjob
{
  isA Job;
  Integer minHours;
  Boolean isLab;
}

class GraderJob
{
  isA Job;
}

class Course
{
  name;
  Integer numTutorialSections;
  Integer numLabSections;
  Integer numStudents;
  
  * courses -- 0..4 Instructor instructors;
  1 course <@>- * Job jobs;
}

class Hours
{
  Date date;
  Time startTime;
  Time endTime;
}

class ResourceManager
{
 singleton;
 0..1 -> * Applicant applicants;
 0..1 -> * Instructor instructors;
 0..1 -> 0..1 Department department;
 0..1 -> 0..1 User loggedIn;
 0..1 -> * Application applications;
 0..1 -> * Job jobs;
 0..1 -> * Course courses;
 0..1 -> * Assignment assignments;
 0..1 -> * Hours contactTimes;
}//$?[End_of_model]$?

